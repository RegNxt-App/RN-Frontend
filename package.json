{
  "name": "@rn/root",
  "version": "1.0.0",
  "description": "Monorepo for Regnxt frontend",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "prettier": "turbo run prettier",
    "prettier-check": "turbo run prettier-check",
    "clean": "turbo run clean && rm -rf node_modules && rm pnpm-lock.yaml"
  },
  "keywords": [
    "monorepo",
    "regnxt",
    "ats",
    "frontend",
    "react",
    "typescript",
    "js",
    "javascript"
  ],
  "author": "abdullahmujahidali",
  "license": "ISC",
  "private": true,
  "packageManager": "pnpm@7.11.0",
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "turbo": "^1.11.3"
  },
  "pnpm": {
    "overrides": {
      "@radix-ui/react-dismissable-layer": "1.0.2"
    }
  }
}
